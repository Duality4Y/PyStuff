from ircBot import ircBot
import BotBrain
import sys

server = "irc.freenode.net"
channel = "#test1123"
nick = "TestAduaD"
botowner = "Duality"

Aduad = ircBot(server,channel,nick,botowner)
Aduad.connect()
brain = BotBrain.BotBrain();

while True:
    data = Aduad.recieve()
    brain.parseCommand(data);
    #if brain.found:
    #    brain.executeCommand();
    print "data >> ",data

sys.exit()